{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "newLine": "lf",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "rootDir": "./source/",
    "strict": true,
    "declaration": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "outDir": "./out/",
    "skipLibCheck": true,
    "target": "es2019"
  },
  "include": ["source/"]
}
